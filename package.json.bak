{
    "name":  "tectangle-gesture-keyboard-mobile",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "description":  "Tectangle gesture keyboard mobile - developer helper scripts",
    "scripts":  {
                    "check-headers":  "node \"August Tectangle Sprint/tectangle-gesture-keyboard-mobile/scripts/check_headers.js\"",
                    "update-meta":  "node \"August Tectangle Sprint/tectangle-gesture-keyboard-mobile/scripts/update_meta_timestamp.js\" --staged",
                    "test-header-meta":  "node --test \"August Tectangle Sprint/tectangle-gesture-keyboard-mobile/tests/unit/header.meta.CameraManager.test.mjs\"",
                    "test-camera-dynamic":  "node --test \"August Tectangle Sprint/tectangle-gesture-keyboard-mobile/tests/unit/camera.dynamic-resolution.test.mjs\"",
                    "test:mocha":  "npx mocha \"**/tests/unit/**/*.mjs\" --exit --reporter spec",
                    "test:tectangle":  "npx mocha \"August Tectangle Sprint/tectangle-gesture-keyboard-mobile/tests/unit/**/*.mjs\" --exit --reporter spec",
                    "test:unit":  "npm run test:mocha",
                    "test:smoke":  "npx mocha \"**/tests/smoke/**/*.mjs\" --exit --reporter spec",
                    "test:golden":  "npx mocha \"**/tests/golden-master/**/*.mjs\" --exit --reporter spec",
                    "test":  "npm run test:unit",
                    "start-smoke-harness":  "node \"August Tectangle Sprint/tectangle-gesture-keyboard-mobile/smoke/camera/smoke-harness.js\"",
                    "smoke:e2e":  "node scripts/smoke-e2e.mjs",
                    "mcp:serve":  "node scripts/run-mcp.js",
                    "prepare":  "husky install",
                    "test:e2e":  "cross-env JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest --config jest.config.cjs",
                    "test:watch:e2e":  "cross-env JEST_PUPPETEER_CONFIG=jest-puppeteer.config.cjs jest --config jest.config.cjs --watchAll",
                    "hive:smoke":  "node HiveFleetObsidian/tools/run_replay_smoke.mjs",
                    "hive:status":  "node HiveFleetObsidian/tools/status.mjs",
                    "hive:smoke:frozen":  "node HiveFleetObsidian/tools/run_frozen_smoke.mjs"
                },
    "lint-staged":  {
                        "*.js":  [
                                     "prettier --write",
                                     "git add"
                                 ],
                        "*.mjs":  [
                                      "prettier --write",
                                      "git add"
                                  ],
                        "*.json":  [
                                       "prettier --write",
                                       "git add"
                                   ],
                        "*.md":  [
                                     "prettier --write",
                                     "git add"
                                 ]
                    },
    "devDependencies":  {
                            "cross-env":  "^7.0.3",
                            "http-server":  "^14.1.1",
                            "husky":  "^8.0.0",
                            "jest":  "^29.7.0",
                            "jest-environment-puppeteer":  "^10.0.1",
                            "jest-puppeteer":  "^10.0.1",
                            "lint-staged":  "^13.0.0",
                            "mocha":  "^10.2.0",
                            "playwright":  "^1.55.0",
                            "prettier":  "^2.8.0",
                            "puppeteer":  "^24.19.0"
                        },
    "engines":  {
                    "node":  "\u003e=18"
                },
    "license":  "MIT"
}

