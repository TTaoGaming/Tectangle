{
  "name": "tectangle-gesture-keyboard-mobile",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Tectangle gesture keyboard mobile - package-level scripts for standalone usage",
  "scripts": {
    "test": "npx mocha \"tests/unit/**/*.mjs\" --exit --reporter spec",
